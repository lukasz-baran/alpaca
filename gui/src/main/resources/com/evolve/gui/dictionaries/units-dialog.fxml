<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="0.0" minWidth="0.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            fx:id="unitsDialog"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.evolve.gui.dictionaries.UnitsController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <TableView fx:id="unitsTable" layoutX="22.0" layoutY="19.0" prefHeight="520.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
            <TableColumn fx:id="unitNumberColumn" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" resizable="false" text="Numer" />
            <TableColumn fx:id="unitDescriptionColumn" prefWidth="220.0" text="Opis" />
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>
</AnchorPane>
